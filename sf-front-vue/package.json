{
  "name": "sf-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean:api": "rimraf src/api",
    "generate:api": "openapi-generator-cli generate -i ../src/main/resources/openapi.yaml -g javascript -o src/api --config openapi-config.json",
    "start": "npm run clean:api && npm run generate:api && npm run dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@videojs/http-streaming": "^3.17.0",
    "axios": "^1.8.4",
    "pinia": "^3.0.2",
    "superagent": "^10.2.1",
    "video.js": "^8.22.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.8.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.20.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "chokidar-cli": "^3.0.0",
    "rimraf": "^6.0.1",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2"
  }
}
